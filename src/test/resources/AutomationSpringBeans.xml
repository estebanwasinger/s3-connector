    <util:map id="bucketPolicyTestData" map-class="java.util.HashMap"
              key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="bucketName" value-ref="randomString"/>
        <entry key="policyTemplate"
               value="{&quot;Version&quot;:&quot;2008-10-17&quot;,&quot;Statement&quot;:[{&quot;Sid&quot;:&quot;AddPerm&quot;,&quot;Effect&quot;:&quot;Allow&quot;,&quot;Principal&quot;:{&quot;AWS&quot;:&quot;*&quot;},&quot;Action&quot;:&quot;s3:GetObject&quot;,&quot;Resource&quot;:&quot;arn:aws:s3:::test_bucket_name/*&quot;}]}"/>
